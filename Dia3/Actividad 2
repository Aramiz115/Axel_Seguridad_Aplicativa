def suma (a,b):
    return a+b 
def resta (a,b):
    return a-b
def multiplicacion (a,b):
    return a*b 
def division (a,b):
    return a/b

while True:
    print("operaciones disponibles")
    print("1.suma")
    print("2.resta")
    print("3.multiplicacion")
    print("4.division")
    operacion = int(input)() 
    
    if (operacion == 1):
        input 
